    <ul class="list2col">
    <% icnt = 1 %>
    <% @mostsalescnty.each do |pavol| %>
      <li><%= icnt.to_s + ":" + pavol.searchparam + " " +  pavol.county %> <li>
      <% icnt += 1 %>
    <% end %>
    </ul>
    </p>
  </a>
  <a href="javascript:void(0)" class="grid-item grid-item-big ">

    <div class="chart-wrapper">
